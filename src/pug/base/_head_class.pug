head
 // Required meta tags
 meta(charset="UTF-8")
 meta(name="viewport", content="width=device-width, initial-scale=1.0, shrink-to-fit=no")
 meta(http-equiv="X-UA-Compatible", content="ie=edge")
 meta(name="og:type", content="Website")
 meta(name="og:site_name", content=titles[0])
 meta(name="og:locale", content="es_ES")
 meta(name="apple-mobile-web-app-title", content="Yogaiskra")
 meta(name="og:author", content="Yogaiskra")
 meta(name="author", content="Yogaiskra")
 meta(name="theme-color", content="#481380")
 
 // Google
 meta(name="googlebot", content="noindex")
 meta(name="googlebot-news", content="noindex")

 // Meta twitter
 meta(name="twitter:card", content="summary")
